package week1.day2;

public class CalculatorReturn {
	
	public int Add(int x, int y)
	{
		return(x+y);
	}
	public double Sub(double a, double b)
	{
		return(a-b);
	}
	public double Mul(double x, double y)
	{
		return(x*y);
	}
	public int Div(int x, int y)
	{
		return(x/y);
	}

	public static void main(String[] args) {
		
		CalculatorReturn cal = new CalculatorReturn();
		int Addition = cal.Add(10,20);
		double Subtraction = cal.Sub(100,20);
		double Multiply = cal.Mul(200,2);
		int Division = cal.Div(300,5);
		System.out.println("Sum of Two numbers=" + Addition);
		System.out.println("Subtraction of Two numbers=" +Subtraction);
		System.out.println("Multiplication of two numbers=" +Multiply);
		System.out.println("Division of two numbers=" +Division);
	}

}
